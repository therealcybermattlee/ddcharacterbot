# Environment variables for D&D Character Manager Frontend
# Copy this file to .env.local and fill in your values

# API Configuration
VITE_API_URL=http://localhost:8787
VITE_API_VERSION=v1

# Application Configuration
VITE_APP_NAME="D&D Character Manager"
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_FEATURE_AI_GENERATION=true
VITE_FEATURE_OFFLINE_MODE=true
VITE_FEATURE_BETA_FEATURES=false

# Analytics (optional)
VITE_GA_TRACKING_ID=
VITE_SENTRY_DSN=

# Development Settings
VITE_DEV_TOOLS=true
VITE_LOG_LEVEL=debug