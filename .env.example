# Security Configuration
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100
CORS_ORIGINS=http://localhost:3000,https://localhost:3000

# Environment
NODE_ENV=development
LOG_LEVEL=debug